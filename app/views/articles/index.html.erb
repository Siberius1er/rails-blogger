<h1>list of articles</h1>
<% @articles.each do |article| %>
    <ul id="list-articles">
      <li><%= link_to article.title, article_path(article) %> </li>
    </ul>
<% end %>
<%= link_to 'List of tags', tags_path %>
<% if logged_in? %>
    <%= link_to 'Create a new Article', new_article_path %>
<% end %>